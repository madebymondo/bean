      "back": "cubic-bezier(0.36, 0, 0.66, -0.56)"
    },
    "out": {
      "sine": "cubic-bezier(0.61, 1, 0.88, 1)",
      "quad": "cubic-bezier(0.5, 1, 0.89, 1)",
      "cubic": "cubic-bezier(0.33, 1, 0.68, 1)",
