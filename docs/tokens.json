    "laptop": {
      "label": "laptop",
      "columns": 12,
      "gap": "40px",
      "max-width": "900px",
      "padding": "40px"
    },
    "desktop": {
      "label": "desktop",
      "columns": 12,
      "gap": "40px",
      "max-width": "1200px",
      "padding": "40px"
    }
  }
