{
  "bp": {
    "phone": "375px",
    "tablet": "768px",
    "laptop": "1024px",
    "desktop": "1440px",
    "ultra-wide": "1920px"
  },
  "font-family": {
    "text": "'Mori', helvetica, arial, sans-serif",
